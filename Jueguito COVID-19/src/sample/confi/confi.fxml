<BorderPane prefHeight="500.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.informacionquecura.Informacionquecura">
   <center>
      <AnchorPane prefHeight="500.0" prefWidth="500.0">
         <children>
            <Canvas fx:id="lienzo" height="398.0" width="600.0" AnchorPane.bottomAnchor="2.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
         </children>
      </AnchorPane>
   </center>
   <bottom>
      <HBox prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <Button mnemonicParsing="false" onAction="#boton7" prefHeight="64.0" prefWidth="128.0" styleClass="jfx-buttonregreso" stylesheets="@../estilos/estilos.css">
               <HBox.margin>
                  <Insets left="10.0" top="20.0" />
               </HBox.margin>
            </Button>
            <Label prefHeight="64.0" prefWidth="187.0" text="Regresar">
               <HBox.margin>
                  <Insets left="20.0" top="20.0" />
               </HBox.margin>
               <font>
                  <Font name="Verdana" size="23.0" />
               </font>
            </Label>
            <HBox prefHeight="100.0" prefWidth="446.0">
               <children>
                  <Label ellipsisString="" prefHeight="64.0" prefWidth="201.0" text="Mostrar mas" textAlignment="RIGHT">
                     <padding>
                        <Insets right="50.0" />
                     </padding>
                     <font>
                        <Font name="Verdana" size="23.0" />
                     </font>
                     <HBox.margin>
                        <Insets left="150.0" top="20.0" />
                     </HBox.margin>
                  </Label>
                  <Button mnemonicParsing="false" onAction="#boton7" prefHeight="64.0" prefWidth="64.0" styleClass="More" stylesheets="@../estilos/estilos.css">
                     <HBox.margin>
                        <Insets left="-50.0" top="20.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
         </children>
      </HBox>
   </bottom>
</BorderPane>
