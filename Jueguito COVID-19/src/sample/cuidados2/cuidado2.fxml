<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<StackPane xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.cuidados2.Cuidados2">
   <children>
      <BorderPane prefHeight="500.0" prefWidth="500.0">
         <bottom>
            <HBox prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER">
               <children>
                  <HBox prefHeight="100.0" prefWidth="250.0">
                     <children>
                        <Button mnemonicParsing="false" onAction="#boton9" prefHeight="64.0" prefWidth="64.0" styleClass="jfx-buttonregreso" stylesheets="@../estilos/estilos.css">
                           <HBox.margin>
                              <Insets left="10.0" top="20.0" />
                           </HBox.margin>
                        </Button>
                        <Label prefHeight="64.0" prefWidth="108.0" text="Regresar">
                           <HBox.margin>
                              <Insets left="20.0" top="20.0" />
                           </HBox.margin>
                           <font>
                              <Font name="Verdana" size="23.0" />
                           </font>
                        </Label>
                     </children>
                  </HBox>
                  <HBox prefHeight="100.0" prefWidth="250.0" />
               </children>
            </HBox>
         </bottom>
         <left>
            <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
               <children>
                  <ImageView fx:id="img1" fitHeight="150.0" fitWidth="150.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../img/hombre.png" />
                     </image>
                  </ImageView>
                  <ImageView fx:id="img2" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../img/tos.png" />
                     </image>
                  </ImageView>
                  <ImageView fx:id="img3" fitHeight="147.0" fitWidth="139.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../img/viajar.png" />
                     </image>
                  </ImageView>
               </children>
            </VBox>
         </left>
         <center>
            <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
               <children>
                  <Label fx:id="lb1" prefHeight="150.0" prefWidth="350.0" text="L1" textAlignment="CENTER" textOverrun="CLIP" wrapText="true">
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                  </Label>
                  <Label fx:id="lb2" prefHeight="150.0" prefWidth="337.0" text="L2" textAlignment="JUSTIFY" textOverrun="CLIP" wrapText="true" />
                  <Label fx:id="lb3" prefHeight="135.0" prefWidth="337.0" text="L3" textAlignment="JUSTIFY" textOverrun="CLIP" wrapText="true" />
               </children>
            </VBox>
         </center>
      </BorderPane>
   </children>
</StackPane>
